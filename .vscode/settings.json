{
    "files.associations": {
        "*.html": "html",
        "*.md": "markdown",
        "*.scss": "scss",
        "*.py": "python"
    },
    "files.exclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true
    },
    "files.watcherExclude": {
            "**/.git/objects/**": true,
            "**/.git/subtree-cache/**": true
    },
    "search.exclude": {
            "**/.git/objects/**": true,
            "**/.git/subtree-cache/**": true
    },
    "search.followSymlinks": false,
    "python.analysis.diagnosticMode": "openFilesOnly"
}
